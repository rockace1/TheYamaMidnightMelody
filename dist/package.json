{
  "name": "melody",
  "version": "0.1.0",
  "private": true,
  "main": "./core/main.js",
  "scripts": {
    "serve-ui": "cross-env NODE_ENV=development vue-cli-service serve",
    "build-ui": "cross-env NODE_ENV=development vue-cli-service build",
    "build-core": "cross-env NODE_ENV=development tsc",
    "build": "vue-cli-service build && tsc && copy package.json dist\\package.json && cd dist && npm i -d",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test": "mocha --timeout 5000 --require ts-node/register ./test/**/*.js",
    "start": "cross-env NODE_ENV=development vue-cli-service build && tsc && electron main.js",
    "dev": "cross-env NODE_ENV=development electron dist/core/main.js",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "pkg": "electron-rebuild -f -w sqlite3 && node build.js"
  },
  "dependencies": {
    "core-js": "^3.6.0",
    "element-ui": "2.13.0",
    "moment": "^2.24.0",
    "sqlite3": "^4.1.1",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "exceljs": "^3.5.0",
    "tslib": "^1.10.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/sqlite3": "^3.1.6",
    "@types/validator": "^12.0.1",
    "@types/exceljs": "^1.3.0",
    "@types/node": "^12.12.22",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-router": "^4.1.1",
    "@vue/cli-plugin-typescript": "^4.1.1",
    "@vue/cli-plugin-unit-mocha": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.30",
    "electron": "^7.1.7",
    "electron-rebuild": "^1.8.8",
    "electron-packager": "^14.1.1",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.5.4",
    "typescript": "~3.7.3",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11"
  }
}
